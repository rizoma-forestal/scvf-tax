<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>mbFamilia</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.especiesforestales.managedBeans.MbFamilia</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>mbGenero</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.especiesforestales.managedBeans.MbGenero</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>mbLogin</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.especiesforestales.managedBeans.MbLogin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>mbEspecie</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.especiesforestales.managedBeans.MbEspecie</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbUsuario</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.especiesforestales.managedBeans.MbUsuario</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
